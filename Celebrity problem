#include <stdio.h>

int findCelebrity(int n, int mat[n][n]) {
    int candidate = 0;

    for (int i = 1; i < n; i++) {
        if (mat[candidate][i] == 1) {
            candidate = i; // candidate knows i, cannot be celebrity
        }
    
    }

    for (int i = 0; i < n; i++) {
        if (i != candidate) {
            if (mat[candidate][i] == 1 || mat[i][candidate] == 0)
                return -1; // fails celebrity conditions
        }
    }

    return candidate;
}

int main() {
    int mat[3][3] = {{1, 1, 0},
                     {0, 1, 0},
                     {0, 1, 1}};

    int n = 3;
    int celeb = findCelebrity(n, mat);
    printf("Celebrity index: %d\n", celeb); // Output: 1

    return 0;
}
